{
  "manifest_version": 3,
  "name": "CADO Extension",
  "description": "This extension takes screenshot of the active tab.",
  "version": "1.0.1",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "icon.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+M",
        "mac": "Command+M"
      }
    }
  }
}
